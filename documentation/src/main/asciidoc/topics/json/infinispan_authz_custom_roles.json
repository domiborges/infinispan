{
  "infinispan" : {
    "cache-container" : {
      "name" : "custom-authorization",
      "security" : {
         "authorization" : {
          "roles" : {
            "myroleone" : {
              "role" : {
                "permissions" : "ALL_WRITE LISTEN"
              }
            },
            "myroletwo" : {
              "role" : {
                "permissions" : "READ WRITE"
              }
            }
          }
        }
      }
    }
  }
}
